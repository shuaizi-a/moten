{
  "name": "moten",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "packageManager": "pnpm@8.10.2",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "commit": "git add . && git-cz",
    "in": "pnpm i -r",
    "ui:build": "pnpm -F @moten/ui build",
    "clear": "rimraf --glob ./node_modules ./packages/**/node_modules ./packages/**/**/node_modules ./packages/**/dist",
    "play:vue3": "pnpm -F @moten/ui build:vue3 && pnpm -F @moten/playground-vue3 dev",
    "play:vue2": "pnpm -F @moten/ui build:vue2 && pnpm -F @moten/playground-vue2 dev",
    "prepare": "husky",
    "lint": "node ./scripts/lint.js"
  },
  "config": {
    "commitizen": {
      "path": "cz-vinyl"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "pnpm lint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "pnpm": {
    "packageExtensions": {
      "vue-template-compiler": {
        "dependencies": {
          "vue": "^2.7.16"
        }
      },
      "vue-template-compiler@2.7.16": {
        "dependencies": {
          "vue": "^2.7.16"
        }
      }
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "commitizen": "^4.3.1",
    "cz-vinyl": "^2.5.4",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.4",
    "rimraf": "^5.0.7",
    "zx": "^8.1.1"
  }
}
